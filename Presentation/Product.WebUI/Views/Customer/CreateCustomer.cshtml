@model CreateCustomerCommand

@{
    ViewData["Title"] = "CreateCustomer";
}

<h1>Yeni Müşteri Ekleme Sayfası</h1>

<form method="post">
    <label>Müşteri Adı ve Soyadı</label>
    <input type="text" asp-for="NameSurname" class="form-control" />
    <br />
    <label>Müşteri Emaili</label>
    <input type="email" asp-for="Email" class="form-control" />
    <br />
    <button class="btn btn-primary">Kaydet</button>
</form>

